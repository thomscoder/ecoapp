{
  "name": "orchestra",
  "version": "1.0.0",
  "main": "server.js",
  "bin": {
    "play": "./cli/config.js"
  },
  "repository": "https://github.com/thomscoder/ecoapp.git",
  "author": "thomscoder <moyelysian@icloud.com>",
  "license": "MIT",
  "scripts": {
    "server": "nodemon -q server.js",
    "client": "cd client && yarn start",
    "settings": "cd cli && node config.js",
    "py-settings": "cd scripts && python3 config.py",
    "dev": "concurrently \"yarn run server\" \"yarn run client\"",
    "build": "cd client && yarn build",
    "postinstall": "cd client && yarn install"
  },
  "dependencies": {
    "@nut-tree/nut-js": "^2.0.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "peer": "^0.6.1",
    "prompt-sync": "^4.2.0",
    "socket.io": "^4.4.1",
    "yarn": "^1.22.17"
  },
  "devDependencies": {
    "concurrently": "7.0.0",
    "dotenv": "14.2.0",
    "nodemon": "2.0.15"
  }
}
